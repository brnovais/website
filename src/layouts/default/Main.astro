---
import BaseLayout from '@layouts/base/Main.astro';
import Content from '@layouts/default/Content.astro';
import Footer from '@layouts/default/Footer.astro';
import Header from '@layouts/default/Header.astro';

const { description, title } = Astro.props;
---

<BaseLayout title={title} description={description}>
  <Header />
  <Content><slot /></Content>
  <Footer />
</BaseLayout>
